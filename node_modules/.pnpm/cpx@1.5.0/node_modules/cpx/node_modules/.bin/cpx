#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/cpx@1.5.0/node_modules/cpx/bin/node_modules:/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/cpx@1.5.0/node_modules/cpx/node_modules:/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/cpx@1.5.0/node_modules:/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/cpx@1.5.0/node_modules/cpx/bin/node_modules:/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/cpx@1.5.0/node_modules/cpx/node_modules:/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/cpx@1.5.0/node_modules:/home/yerchie/WebstormProjects/EduAPI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
